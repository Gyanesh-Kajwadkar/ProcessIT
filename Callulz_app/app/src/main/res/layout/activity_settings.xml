<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/background"
    tools:context=".Activity.setting_area.SettingsActivity">

    <include layout="@layout/tab_layout"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">


        <RelativeLayout
            android:id="@+id/printer_ll"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:padding="20dp">

            <TextView
                android:id="@+id/printename_tv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginRight="80dp"
                android:layout_alignParentLeft="true"
                android:layout_centerInParent="true"
                android:textColor="@color/black"
                android:text="Printers"
                android:gravity="left|center"
                android:textSize="14sp"/>
            <ImageView
                android:layout_width="25dp"
                android:layout_height="22dp"
                android:layout_alignParentRight="true"
                android:layout_marginRight="0dp"
               android:src="@drawable/right" />
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/black"/>
        <RelativeLayout
            android:id="@+id/config_ll"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:padding="20dp">

            <TextView
                android:id="@+id/config_tv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginRight="80dp"
                android:layout_alignParentLeft="true"
                android:layout_centerInParent="true"
                android:textColor="@color/black"
                android:text="Configurations"
                android:gravity="left|center"
                android:textSize="14sp"/>

            <ImageView
                android:layout_width="25dp"
                android:layout_height="22dp"
                android:layout_alignParentRight="true"
                android:layout_marginRight="0dp"
                android:src="@drawable/right" />
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/black"/>
        <RelativeLayout
            android:id="@+id/remot_ll"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:padding="20dp">

            <TextView
                android:id="@+id/remot_tv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginRight="80dp"
                android:layout_alignParentLeft="true"
                android:layout_centerInParent="true"
                android:textColor="@color/black"
                android:text="Remote Support"
                android:gravity="left|center"
                android:textSize="14sp"/>

            <ImageView
                android:layout_width="25dp"
                android:layout_height="22dp"
                android:layout_alignParentRight="true"
                android:layout_marginRight="0dp"
                android:src="@drawable/right" />
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/black"/>
        <RelativeLayout
            android:id="@+id/about_ll"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:padding="20dp">

            <TextView
                android:id="@+id/about_tv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginRight="80dp"
                android:textColor="@color/black"
                android:text="About"
                android:gravity="left|center"
                android:textSize="14sp"/>
            <TextView
                android:id="@+id/aboutValue_tv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/about_tv"
                android:layout_marginTop="0dp"
                android:text="3.0.65"
                android:gravity="left|center"
                android:textColor="@color/dark_grey"
                android:textSize="14sp"/>

            <ImageView
                android:layout_width="25dp"
                android:layout_height="22dp"
                android:layout_alignParentRight="true"
                android:layout_marginRight="0dp"
                android:src="@drawable/right" />
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/black"/>


    </LinearLayout>

</LinearLayout>